{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "ConnectionStrings": {
    "postgres": "Host=localhost;Port=59248;Database=db;Username=postgres;Password=123456"
  },
  "NOF": {
    "Authority": {
      "Issuer": "NOF.Sample",
      "KeySize": 2048,
      "RetiredKeyRetentionCount": 2,
      "KeyRotationInterval": "30.00:00:00"
    },
    "JwtClient": {
      "Authority": "https://auth.example.com",
      "RequireHttpsMetadata": true,
      "Issuer": null,
      "Audience": null,
      "CacheLifetime": "30.00:00:00"
    },
    "Authorization": {
      "HeaderName": "Authorization",
      "TokenType": "Bearer"
    },
    "Outbox": {
      "PollingInterval": "00:00:05",
      "BatchSize": 100,
      "MaxRetryCount": 5,
      "ClaimTimeout": "00:05:00"
    },
    "DbContextFactory": {
      "AutoMigrate": false
    },
    "HttpHeaderMiddleware": {
      "BlacklistedPatterns": [ "NOF.*" ]
    },
    "CorsSettings": {
      "AllowedOrigins": [],
      "AllowedMethods": [ "GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS" ],
      "AllowedHeaders": [ "*" ],
      "AllowCredentials": true
    }
  }
}
