<Project>
  <PropertyGroup>
    <!-- Common Package Properties -->
    <Authors>XmmShp</Authors>
    <Company>NOF</Company>
    <Product>NOF Framework</Product>
    <Copyright>Copyright Â© $(Company) $([System.DateTime]::Now.Year)</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/XmmShp/NOF</PackageProjectUrl>
    <RepositoryUrl>https://github.com/XmmShp/NOF</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>nof;framework;dotnet;aspnetcore</PackageTags>
    
    <!-- Version -->
    <VersionPrefix>2.1.0</VersionPrefix>
    <VersionSuffix Condition="'$(Configuration)' == 'Debug'">dev</VersionSuffix>
    
    <!-- Build Properties -->
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <ContinuousIntegrationBuild Condition="'$(CI)' == 'true'">true</ContinuousIntegrationBuild>
    
    <!-- Symbol Package -->
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>
